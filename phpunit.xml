<?xml version="1.0" encoding="UTF-8"?>
<phpunit xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://schema.phpunit.de/|version|/phpunit.xsd">
    <testsuites>
        <testsuite name="unit">
            <directory phpVersion="8.2.0" phpVersionOperator=">=">test/unit</directory>
        </testsuite>
        <testsuite name="integration">
            <directory phpVersion="8.2.0" phpVersionOperator=">=">test/integration</directory>
        </testsuite>
    </testsuites>
    <logging>
        <testdoxHtml outputFile="reports/testdox.html" />
        <testdoxText outputFile="reports/testdox.txt" />
    </logging>
</phpunit>